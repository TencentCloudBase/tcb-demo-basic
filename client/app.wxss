}

.tcb-srcoll-box {
  position: relative;
  overflow: hidden;
  display: flex;
  flex-direction: column;
  height: 100vh;
}
::-webkit-scrollbar {
  display:none;
  width:0;
  height:0;
  color:transparent;
}
.nav_bar_customize_top{
  position: sticky;
  top: 0;
  z-index: 9;
  width: 100%;    
  overflow: hidden;
}
.tcb-container {
  overflow-y: scroll;
  overflow-x: hidden;
  flex: 1;
  z-index: 0;
}


.section {
  overflow: hidden;
  padding: 36rpx 32rpx;
}
.section__title {
  font-size: 36rpx;
  font-weight: 500;
  line-height: 56rpx;
}
.tcb-tabs__bar {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 128rpx;
}
.tcb-tabs__item {
  width: 212rpx;
  height: 64rpx;  
  border: none;
  border-radius: 32rpx;
  background-color: #F4F5F9;
  color: #757D88;
  font-size: 26rpx !important;
  line-height: 64rpx !important;
  text-align: center;
  margin: 0;
}
.tcb-tabs__item::after {
  border: none;
}
.tcb-tabs__item--active {
  background-color: #006EFF;
  color: #fff;
  font-size: 500;
}

.tcb-card {
  position: relative;
  overflow: hidden;
  background-color: #fff;
  border-radius: 20rpx;
  margin-bottom: 32rpx;
}
.tcb-card__logo {
  height: 300rpx;
}
.tcb-card__logo image {
  width: 100%;
  height: 100%;
}
.tcb-card__content {
  padding: 32rpx 26rpx 48rpx;
}
.tcb-card_icon {
  width: 48rpx;
  height: 48rpx;
  margin-right: 16rpx;
}
.tcb-card__title {
  display: flex;
  align-items: center;
  font-size: 32rpx;
  font-weight: 500;
  line-height: 48rpx;
}
.tcb-card__description {
  position: relative;
  font-size: 28rpx;
  line-height: 40rpx;
  color: #757D88;
  margin-top: 20rpx;
}