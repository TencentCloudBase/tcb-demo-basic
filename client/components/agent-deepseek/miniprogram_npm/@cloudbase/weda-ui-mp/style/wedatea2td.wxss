.wedatea2td-icon {
  width: 16px;
  height: 16px;
  display: inline-block;
  vertical-align: middle;
  background-repeat: no-repeat;
  background-position: inherit;
  font-size: 0;
}

.wedatea2td-action-state {
  font-size: 14px;
  display: inline-flex;
  max-width: 100%;
  align-items: center;
}
.wedatea2td-mr-2n {
  margin-right: 8px !important;
}

.wedatea2td-action-state__text {
  display: inline;
}

.wedatea2td-icon-loading {
  background-image: url(data:image/svg+xml;base64,);
  -webkit-animation: spin 0.6s linear infinite;
  animation: spin 0.6s linear infinite;
}

.wedatea2td-icon-loading {
  background: conic-gradient(from 90deg at 50% 50%, #fff 0deg, #0052d9 1turn);
  border-radius: 50%;
  -webkit-mask: radial-gradient(transparent calc(50% - 0.5px), #fff 50%);
  mask: radial-gradient(transparent calc(50% - 0.5px), #fff 50%);
}

@-webkit-keyframes spin {
  0% {
    -webkit-transform: rotate(0deg);
  }

  to {
    -webkit-transform: rotate(359deg);
  }
}

@keyframes spin {
  0% {
    transform: rotate(0deg);
  }

  to {
    transform: rotate(359deg);
  }
}
